using namespace std;
const int maxn=1e6+1;
ll a[maxn+7];
int main(){
	freopen("a.inp","r",stdin);
	int n=1e6;
	forr(i,1,n){
		cin>>a[i];
	}
	clock_t start = clock();
    sort(a+1,a+1+n);
	clock_t end   = clock();
    double time_spent = (double)(end - start) / CLOCKS_PER_SEC;
    cout<<time_spent;

}
